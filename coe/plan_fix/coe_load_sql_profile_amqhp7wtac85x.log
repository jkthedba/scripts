 10  	 :other_xml := i.other_xml;
 11  	 EXIT; -- 1st
 12    END LOOP;
 13  EXCEPTION
 14    WHEN OTHERS THEN
 15  	 DBMS_OUTPUT.PUT_LINE('getting modified other_xml from memory: '||SQLERRM);
 16  	 :other_xml := NULL;
 17  END;
 18  /
